{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "start:test": "cross-env BROWSER=none REACT_APP_IS_TEST=Y react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:it": "cypress run",
    "test:e2e": "cross-env CYPRESS_E2E=Y cypress run",
    "test:it:edge": "cypress run --browser edge",
    "test:e2e:edge": "cross-env CYPRESS_E2E=Y cypress run --browser edge",
    "cypress:open": "cypress open",
    "cypress:open:e2e": "cross-env CYPRESS_E2E=Y start-server-and-test start:test http://localhost:3000 cypress:open"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/support/steps"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "fontsource-roboto": "^2.2.6",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "cypress": "^5.1.0",
    "cypress-cucumber-preprocessor": "^2.5.4",
    "cypress-wait-until": "^1.7.1",
    "start-server-and-test": "^1.11.3"
  }
}
